<%= form_for @subpage do |f| %>
  <p>
    <%= f.label "Tittle" %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label "Innhold" %><br />
    <%= f.text_area :content,:class => "tinymce", :rows => 20, :cols => 65 %>
    <%= tinymce %>
  </p>
  <% Submenubutton.find(:all).each do |s|%>
  	<%= f.radio_button :submenubutton_id, s.id %>
  	<%= label_tag s.title%>
  <% end%>
  
  <p><%= f.submit "Lagre" %></p>
<% end %>
